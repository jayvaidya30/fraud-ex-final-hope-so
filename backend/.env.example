ENVIRONMENT=dev
# IMPORTANT: Put special characters ONLY in the password part and URL-encode them (don't modify the hostname).
# Example:
# DATABASE_URL=postgresql+psycopg://postgres:YOUR_URLENCODED_PASSWORD@db.YOUR_PROJECT_REF.supabase.co:5432/postgres
DATABASE_URL=postgresql+psycopg://postgres:YOUR_URLENCODED_PASSWORD@db.YOUR_PROJECT_REF.supabase.co:5432/postgres
JWT_SECRET_KEY=change-me
JWT_ALGORITHM=HS256
# Preferred Gemini key name (backend also accepts API_KEY for legacy setups)
GEMINI_API_KEY=

SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co
SUPABASE_ANON_KEY=
SUPABASE_JWT_AUDIENCE=authenticated
# Optional: override issuer if needed (defaults to {SUPABASE_URL}/auth/v1)
SUPABASE_JWT_ISSUER=
SUPABASE_JWT_SECRET=
